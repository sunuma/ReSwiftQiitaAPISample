  target 'TekitoReduxSampleTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxBlocking'
    pod 'RxTest'
  end

  target 'TekitoReduxSampleUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
